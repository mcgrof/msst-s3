# Moderate Parallel Test Configuration
# Runs tests in parallel with controlled concurrency

# S3 endpoint for MinIO Docker container
CONFIG_S3_ENDPOINT_URL="http://localhost:9000"
CONFIG_S3_ACCESS_KEY="minioadmin"
CONFIG_S3_SECRET_KEY="minioadmin"
CONFIG_S3_REGION="us-east-1"
CONFIG_S3_BUCKET_PREFIX="msst-parallel-mod"

# Enable basic test categories
CONFIG_TEST_BASIC=y
CONFIG_TEST_MULTIPART=y
CONFIG_TEST_VERSIONING=y
CONFIG_TEST_ACL=n
CONFIG_TEST_ENCRYPTION=n
CONFIG_TEST_LIFECYCLE=n
CONFIG_TEST_PERFORMANCE=n
CONFIG_TEST_STRESS=n
CONFIG_TEST_COMPATIBILITY=n

# Parallel mode with limited concurrency
CONFIG_TEST_RUN_MODE="parallel"
CONFIG_TEST_PARALLEL_ALL=n
CONFIG_TEST_PARALLEL_JOBS=10

# Test parameters
CONFIG_TEST_TIMEOUT=300
CONFIG_TEST_RETRY_COUNT=2
CONFIG_TEST_OBJECT_SIZES="1KB,1MB"
CONFIG_TEST_BUCKET_COUNT=3
CONFIG_TEST_OBJECT_COUNT=5

# Output configuration
CONFIG_OUTPUT_FORMAT="json"
CONFIG_OUTPUT_VERBOSE=1
CONFIG_OUTPUT_DIR="./results/parallel-moderate"
CONFIG_OUTPUT_TIMESTAMP=y
CONFIG_OUTPUT_SAVE_LOGS=y

# Vendor configuration
CONFIG_VENDOR_TYPE="minio"

# Developer options
CONFIG_DEBUG_ENABLED=n
CONFIG_DEBUG_KEEP_BUCKETS=n